#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspaces/codespaces-blank/node_modules/.pnpm/graphql-request@7.1.0_graphql@16.9.0/node_modules/graphql-request/build/cli/node_modules:/workspaces/codespaces-blank/node_modules/.pnpm/graphql-request@7.1.0_graphql@16.9.0/node_modules/graphql-request/build/node_modules:/workspaces/codespaces-blank/node_modules/.pnpm/graphql-request@7.1.0_graphql@16.9.0/node_modules/graphql-request/node_modules:/workspaces/codespaces-blank/node_modules/.pnpm/graphql-request@7.1.0_graphql@16.9.0/node_modules:/workspaces/codespaces-blank/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspaces/codespaces-blank/node_modules/.pnpm/graphql-request@7.1.0_graphql@16.9.0/node_modules/graphql-request/build/cli/node_modules:/workspaces/codespaces-blank/node_modules/.pnpm/graphql-request@7.1.0_graphql@16.9.0/node_modules/graphql-request/build/node_modules:/workspaces/codespaces-blank/node_modules/.pnpm/graphql-request@7.1.0_graphql@16.9.0/node_modules/graphql-request/node_modules:/workspaces/codespaces-blank/node_modules/.pnpm/graphql-request@7.1.0_graphql@16.9.0/node_modules:/workspaces/codespaces-blank/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../graphql-request/build/cli/generate.js" "$@"
else
  exec node  "$basedir/../graphql-request/build/cli/generate.js" "$@"
fi
